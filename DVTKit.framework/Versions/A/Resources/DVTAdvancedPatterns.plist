<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<array>
	<dict>
		<key>id</key>
		<string>Email Address</string>
		<key>regularExpression</key>
		<string>\b[A-Za-z0-9._%-]+@[A-Za-z0-9.-]+\.[A-Za-z]{2,4}\b</string>
		<key>allowsBackreferences</key>
		<true/>
	</dict>
	<dict>
		<key>id</key>
		<string>Web Address</string>
		<key>regularExpression</key>
		<string>(?:[A-Za-z][A-Za-z0-9+.-]{1,120}:[A-Za-z0-9/](?:(?:[A-Za-z0-9$_.+!*,;/?:@&amp;~=-])|%[A-Fa-f0-9]{2}){1,333}(?:#(?:[a-zA-Z0-9][a-zA-Z0-9$_.+!*,;/?:@&amp;~=%-]{0,1000}))?)</string>
		<key>allowsBackreferences</key>
		<true/>
	</dict>
	<dict>
		<key>id</key>
		<string>IP Address</string>
		<key>regularExpression</key>
		<string>(?:\d{1,3}(?:\.\d{1,3}){3}|[:xdigit:]{1,4}(?::[:xdigit:]{1,4}){7})</string>
		<key>allowsBackreferences</key>
		<true/>
	</dict>
</array>
</plist>
